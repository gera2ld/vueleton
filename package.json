{
  "name": "vueleton",
  "version": "1.0.5",
  "description": "Vue components with only core features.",
  "author": "Gerald <i@gerald.top>",
  "license": "ISC",
  "homepage": "https://github.com/gera2ld/vueleton#readme",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "del-cli lib",
    "dev": "rollup -wc rollup.conf.js",
    "build:js": "rollup -c rollup.conf.js",
    "build": "cross-env NODE_ENV=production run-s clean ci build:js",
    "prepublishOnly": "run-s build",
    "lint": "eslint --ext .js,.vue .",
    "ci": "run-s lint",
    "start:docs": "cd docs && nuxt",
    "build:docs": "cd docs && nuxt generate",
    "upload:docs": "gh-pages -d docs/dist",
    "deploy": "run-s build:docs upload:docs"
  },
  "keywords": [
    "vue",
    "skeleton"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "codemirror": "5.x",
    "vue": "2.x"
  },
  "devDependencies": {
    "@gera2ld/plaid": "~2.3.0",
    "@gera2ld/plaid-common-vue": "~2.2.0",
    "@gera2ld/plaid-rollup": "~2.3.2",
    "codemirror": "^5.55.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.23.0",
    "gh-pages": "^3.1.0",
    "highlight.js": "^10.1.1",
    "marked": "^1.1.0",
    "nuxt": "^2.13.3",
    "raw-loader": "^4.0.1",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gera2ld/vueleton.git"
  },
  "bugs": {
    "url": "https://github.com/gera2ld/vueleton/issues"
  }
}
